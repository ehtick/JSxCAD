// Probably from https://www.pinterest.de/pin/200410252158492924/
(await readSvg('https://jsxcad.js.org/svg/dinosaur.svg', { fill: false }))
  .scale(10)
  .align('xy')
  .and(toolpath())
  .gcode('dinosaur');

Spiral((a) => [[1 + (a * 360) / 100]], {
  to: 10,
  by: 3 / 360,
}).and(toolpath())
  .gcode('spiral');

(await readSvg('https://jsxcad.js.org/svg/flowers.svg', { fill: false }))
  .scale(1 / 60)
  .align()
  .and(toolpath())
  .gcode('flowers');

(await readSvg('https://jsxcad.js.org/svg/mandala.svg', { fill: false }))
  .align('xy')
  .scale(20)
  .and(toolpath())
  .gcode('mandala');

(await readSvg('https://jsxcad.js.org/svg/visnezh.svg', { fill: false }))
  .align('xy')
  .scale(1 / 100)
  .and(toolpath())
  .gcode('visnezh');

Group(
  seq(
    (x) =>
      Box(10)
        .x(x)
        .op((s) =>
          Group(seq((y) => s.y(y), { from: -100, upto: 100, by: 60 }))
        ),
    { from: -100, upto: 100, by: 60 }
  )
)
  .align('xy')
  .toolpath()
  .gcode('calibration');

const star = Triangle(5)
  .seq({ upto: 120 / 360, by: 30 / 360 }, rz, Join)
  .view();

const r = Random();

const stars = star
  .seq(
    { upto: 20 },
    () => (s) => s.x(r.in(-100, 100)).y(r.in(-100, 100)),
    Group
  )
  .and(toolpath())
  .gcode('stars');
